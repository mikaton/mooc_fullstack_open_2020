{
    "env": {
        "browser": true,
        "jest": true,
        "es6": true
    },
    "extends": ["plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "rules": {
      "prettier/prettier": ["error"],
      "no-console": "off"
    }
  }